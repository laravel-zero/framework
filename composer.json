{
    "name": "laravel-zero/framework",
    "description": "The Laravel Zero Framework.",
    "keywords": ["framework", "laravel", "console", "zero", "cli"],
    "homepage": "http://laravel-zero.com",
    "license": "MIT",
    "support": {
        "issues": "https://github.com/laravel-zero/laravel-zero/issues",
        "source": "https://github.com/laravel-zero/laravel-zero"
    },
    "authors": [
        {
            "name": "Nuno Maduro",
            "email": "enunomaduro@gmail.com"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "league/flysystem": "1.0.*",
        "illuminate/cache": "5.6.*",
        "illuminate/config": "5.6.*",
        "illuminate/console": "5.6.*",
        "illuminate/container": "5.6.*",
        "illuminate/events": "5.6.*",
        "illuminate/filesystem": "5.6.*",
        "laravel-zero/foundation": "5.6.*",
        "mtdowling/cron-expression": "1.2.*",
        "nunomaduro/collision": "dev-release-2.0.0",
        "nunomaduro/laravel-console-task": "1.0.*",
        "nunomaduro/laravel-console-menu":"1.0.*",
        "nunomaduro/laravel-desktop-notifier": "2.0.*",
        "nunomaduro/laravel-console-summary": "1.0.*",
        "symfony/debug": "4.0.*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require-dev": {
        "phpunit/phpunit": "~7.0",
        "illuminate/database": "5.6.*",
        "illuminate/log": "5.6.*"
    },
    "autoload": {
        "psr-4": {
            "LaravelZero\\Framework\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests",
            "App\\": "tests/Application/app"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    },
    "scripts": {
        "test": "phpunit"
    }
}
